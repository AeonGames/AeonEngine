{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "/usr/include",
                "/usr/local/include",
                "${workspaceRoot}"
            ],
            "defines": [],
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "/usr/include",
                    "/usr/local/include",
                    "${workspaceRoot}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ]
        },
        {
            "name": "Linux",
            "includePath": [
                "/usr/include/c++/5.4.0",
                "/usr/include/x86_64-linux-gnu/c++/5.4.0",
                "/usr/local/include",
                "/usr/lib/clang/3.8.0/include",
                "/usr/include",
                "/usr/include/x86_64-linux-gnu",
                "${workspaceRoot}"
            ],
            "defines": [
                "__GNUC__",
                "__unix__"
            ],
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "/usr/include/c++/5.4.0",
                    "/usr/include/x86_64-linux-gnu/c++/5.4.0",
                    "/usr/local/include",
                    "/usr/lib/clang/3.8.0/include",
                    "/usr/include",
                    "/usr/include/x86_64-linux-gnu",
                    "${workspaceRoot}/../include", 
                    "${workspaceRoot}/engine/include", 
                    "${workspaceRoot}/engine/core",
                    "${workspaceRoot}/engine/renderers/opengl",
                    "${workspaceRoot}/engine/renderers/vulkan",
                    "${workspaceRoot}/engine/renderers/vulkan/SPIR-V"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "",
                "compileCommands": "@CMAKE_BINARY_DIR@/compile_commands.json"
            }
        },
        {
            "name": "Windows",
            "includePath": [
                "${workspaceRoot}/include",
                @INCLIST@
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "WIN32",
                "_WIN32",
                "WINDOWS",
                "__GNUC__",
                "__MINGW32__",
                "__MINGW64__",
                "DLL=",
                "QT_STATIC"
            ],
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceRoot}/include",
                    @INCLIST@
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "compileCommands": "@CMAKE_BINARY_DIR@/compile_commands.json",
            "codegnuglobal.executable": "@GLOBAL_EXECUTABLE@"
        }
    ],
    "version": 3
}
