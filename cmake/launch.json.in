                "environment": [
                    {
                        "name":"PATH",
                        "value":"@DEBUG_PATH@"
                    }                    
                ],
                "externalConsole": @USE_EXTERNAL_CONSOLE@,
                "program": "@CMAKE_BINARY_DIR@/bin/game@CMAKE_EXECUTABLE_SUFFIX@",
                "miDebuggerPath": "@GDB_EXECUTABLE@",
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
