                "stopAtEntry": false,
                "cwd": "@CMAKE_SOURCE_DIR@",
                "environment": [
                    {
                        "name":"PATH",
                        "value":"@DEBUG_PATH@"
                    }                    
                ],
                "externalConsole": @USE_EXTERNAL_CONSOLE@,
